      <None Remove="Assets\Fonts\Raleway-Medium.ttf" />
      <None Remove="Assets\Fonts\Raleway-SemiBold.ttf" />
      <None Remove="Assets\Fonts\Roboto-Medium.ttf" />
      <None Remove="Assets\Fonts\Roboto-Regular.ttf" />
      <None Remove="Assets\Fonts\Saira-Regular.ttf" />
      <None Remove="Assets\Fonts\Saira-SemiBold.ttf" />
      <None Remove="Assets\icon.ico" />
      <None Remove="Assets\icon.png" />
      <None Remove="Assets\shutdown_def.png" />
      <None Remove="Assets\shutdown_mo.png" />
    </ItemGroup>

    <ItemGroup>
      <Resource Include="Assets\bg.png" />
