<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>WinExe</OutputType>
        <TargetFramework>net8.0-windows</TargetFramework>
        <RootNamespace>VRAutiverse</RootNamespace>
        <Nullable>enable</Nullable>
        <UseWPF>true</UseWPF>
